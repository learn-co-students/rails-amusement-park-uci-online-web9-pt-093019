<h1><%= @user.name %></h1>

<h2>Mood: <%= @user.mood %></h2>

<%= form_for @user do |f| %>
    
    <p><%= f.label :password %> <%= f.text_field :password %></p>
    <p><%= f.label :height %> <%= f.text_field :height %></p>
    <p><%= f.label :happiness %> <%= f.text_field :happiness %></p>
    <p><%= f.label :nausea %> <%= f.text_field :nausea %></p>
    <p><%= f.label :tickets %> <%= f.text_field :tickets %></p>
    <%= f.submit %>

<% end %>